-# This file is  app/views/joblisting/index.html.haml
%h2 Available Jobs

%table#job
  %thead
    %tr
      %th Company Name
      %th Job Description
      %th Language Requirements
      %th Contact Name
      %th Contact Number
      %th Contact Email
  %tbody
    - @jobs.each do |item|
      %tr
        %td= item.companyName
        %td= item.jobDescription
        %td= item.language
        %td= item.contactName
        %td= item.contactNumber
        %td= item.contactEmail
        
= link_to 'Add new job listing', new_joblisting_path

        